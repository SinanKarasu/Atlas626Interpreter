 000001 BEGIN, ATLAS PROGRAM 'TRY' $
     70 DECLARE, VARIABLE, 'X' IS DECIMAL INITIAL=1.1 $
        DECLARE,VARIABLE, 'I' IS INTEGER  INITIAL = 0 $
 099000 COMMENCE, MAIN PROCEDURE $
C        CALCULATE, 'X' =1      $
